<?xml version="1.0"?>
<launch>

<rosparam file="$(find antrop_arms_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="antrop_arms" args="base_shoulder_left_joint_position_controller shoulder_elbow_left_joint_position_controller base_shoulder_right_joint_position_controller shoulder_elbow_right_joint_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/antrop_arms/joint_states"/>
</node>

</launch>
